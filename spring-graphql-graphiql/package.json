{
  "name": "example-graphiql-webpack",
  "version": "1.1.1-alpha.8",
  "private": true,
  "license": "MIT",
  "description": "A GraphiQL example with webpack and typescript",
  "scripts": {
    "mavenbuild": "webpack-cli",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "format": "prettier --write **/*.ts --write **/*.tsx",
    "format-check": "prettier --check **/*.ts --check **/*.tsx",
    "bundle-report": "mkdir -p dist && webpack --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "graphql": "16.0.1",
    "graphiql": "1.5.5",
    "@graphiql/toolkit": "0.4.2",
    "react": "17.0.2",
    "graphql-ws": "^5.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/react-dom": "^17.0.11",
    "@types/regenerator-runtime": "^0.13.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.1",
    "html-webpack-plugin": "^4.2.0",
    "prettier": "^1.19.1",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.20",
    "style-loader": "^1.1.3",
    "typescript": "^4.5.2",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "worker-loader": "^2.0.0"
  }
}
